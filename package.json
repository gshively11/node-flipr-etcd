{
  "name": "flipr-etcd",
  "version": "1.0.0",
  "description": "Flipr source to retrieve config from etcd:  a distributed, consistent, partition-tolerant key/value store",
  "main": "lib/flipr-etcd.js",
  "bin": {
    "jshint": "node_modules/jshint/bin/jshint",
    "mocha": "node_modules/mocha/bin/mocha"
  },
  "scripts": {
    "jshint": "jshint lib test",
    "debug": "mocha --debug-brk",
    "cover": "istanbul cover ./node_modules/mocha/bin/_mocha",
    "report": "istanbul report cobertura",
    "test": "npm run-script jshint && npm run-script cover && npm run-script report"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:gshively11/node-flipr-etcd.git"
  },
  "keywords": [
    "flipr",
    "source",
    "etcd"
  ],
  "author": {
    "name": "Grant Shively",
    "email": "gshively11@gmail.com"
  },
  "homepage": "https://github.com/gshively11/node-flipr-etcd",
  "bugs": "https://github.com/gshively11/node-flipr-etcd/issues",
  "license": "MIT",
  "devDependencies": {
    "chai": "^1.9.1",
    "istanbul": "^0.2.16",
    "jshint": "^2.5.2",
    "mocha": "^1.20.1",
    "mocha-istanbul": "^0.2.0",
    "proxyquire": "^1.0.1",
    "sinon": "^1.10.2",
    "sinon-chai": "^2.5.0"
  },
  "dependencies": {
    "lodash": "^2.4.1",
    "memoizee": "^0.3.7",
    "node-etcd": "^2.1.4"
  }
}
